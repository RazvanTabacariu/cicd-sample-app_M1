node {
    stage('Preparation') {
        checkout scm  // <-- pulls the latest commit
        catchError(buildResult: 'SUCCESS') {
            sh 'docker stop samplerunning || true'
            sh 'docker rm samplerunning || true'
        }
    }
    stage('Build') {
        build 'SampleApp'
    }
    stage('Results') {
        build 'SampleAppCURL'
    }
}
